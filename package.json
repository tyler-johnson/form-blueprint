  "devDependencies": {
    "@pagedip/tool-autorelease": "^3.5.4",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.6",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
